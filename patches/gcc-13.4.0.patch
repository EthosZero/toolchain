diff -ruN clean/gcc-13.4.0/gcc/config/ethos.h dirty/gcc-13.4.0/gcc/config/ethos.h
--- clean/gcc-13.4.0/gcc/config/ethos.h	1969-12-31 19:00:00.000000000 -0500
+++ dirty/gcc-13.4.0/gcc/config/ethos.h	2025-09-07 00:12:02.226550654 -0400
@@ -0,0 +1,26 @@
+#undef TARGET_ETHOS_H
+#define TARGET_ETHOS_H 1
+
+/* Where to link against by default */
+#undef LIB_SPEC
+#define LIB_SPEC "-lc"
+
+/* Stuff linked before user code */
+#undef STARTFILE_SPEC
+#define STARTFILE_SPEC ""
+
+/* Stuff linked after user code */
+#undef ENDFILE_SPEC
+#define ENDFILE_SPEC ""
+
+/* Where lib stuff is */
+#undef STANDARD_STARTFILE_PREFIX
+#define STANDARD_STARTFILE_PREFIX "/lib/"
+
+#undef TARGET_OS_CPP_BUILTINS
+#define TARGET_OS_CPP_BUILTINS()		\
+	do {								\
+		builtin_define("__ethos__");	\
+		builtin_define("__unix__");		\
+		builtin_define("system=ethos"); \
+	} while (0);
